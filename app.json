{
  "expo": {
    "name": "PlayRush",
    "slug": "PlayRush",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icone.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "esportesnews",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icone.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lorena_assoni.PlayRush"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icone.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icone.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "router": {
        "staticRendering": false
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ef0cf0f7-907d-4050-a9bd-031973317320"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ef0cf0f7-907d-4050-a9bd-031973317320"
    }
  }
}
